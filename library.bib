@article{skuhravaDipteraChapter102010,
  title = {Diptera. {{Chapter}} 10},
  volume = {4},
  issn = {1313-2652},
  doi = {10.3897/biorisk.4.53},
  abstract = {Of the 19,400 native species and 125 families forming the European diptera fauna, 98 species (less than 0.5\%) in 22 families are alien to Europe. Th ese aliens constitute 66 species (18 families) of the suborder Brachycera and 32 species (4 families) of the suborder Nematocera. By family in this category, there are 23 Cecidomyiidae species, 18 Drosophilidae, nine Phoridae, eight Tachinidae and seven Culicidae. Another 32 fl y species belonging to fi ve families are considered to be alien in Europe. Th ese invasives native to other European countries are composed of 14 species of Cecidomyiidae, seven Syrphidae, fi ve Culicidae and three species each of Anthomyiidae and Tephritidae. Th e date of the fi rst record in Europe is known for 84 alien species. Arrivals of alien species of Diptera have accelerated rapidly since the second half of the 20 th century. North America appears to be the dominant contributor of the alien fl ies. Th e majority of alien Diptera were introduced into or within Europe unintentionally, with only three predators released intentionally for biological control. Alien Diptera are predominantly phytophagous (35.6\%), while a lesser portion are zoophagous (28.6\%) or detrivorous /mycetophagous (29.6\%). Ecological impacts on native fauna and fl ora have not been documented for any of the alien species established in Europe. How-ever, 14 alien species have economic impacts on crops.},
  number = {2},
  journal = {BioRisk},
  author = {Skuhrav\'a, Marcela and Martinez, Michel and Roques, Alain},
  year = {2010},
  pages = {553--602}
}

@article{herrickAcousticalDetectionEarly2012,
  title = {Acoustical {{Detection}} of {{Early Instar Rhynchophorus Ferrugineus}} ({{Coleoptera}}: {{Curculionidae}}) in {{Canary Island Date Palm}}, {{Phoenix Canariensis}} ({{Arecales}}: {{Arecaceae}})},
  volume = {95},
  issn = {0015-4040},
  doi = {10.1653/024.095.0425},
  abstract = {The red palm weevil (RPW), Rhynchophorus ferrugineus (Olivier), recently\$$\backslash$backslash\$nfound in Curacao and Aruba, has become an economically significant palm\$$\backslash$backslash\$ntree pest in many tropical and subtropical regions. By the time a palm\$$\backslash$backslash\$ninfested with RPW displays visible damage, larvae have destroyed much of\$$\backslash$backslash\$nthe trunk internal structure, typically resulting in tree mortality.\$$\backslash$backslash\$nAcoustic technology may enable pest managers to detect and treat early\$$\backslash$backslash\$nRPW infestations before tree mortality, and to reduce unwanted\$$\backslash$backslash\$nimportation and/or exportation of infested palms. Experiments were\$$\backslash$backslash\$nconducted in Aruba to determine the detectability of sounds produced by\$$\backslash$backslash\$nearly instars in open, urban environments and in enclosures with ca. 10\$$\backslash$backslash\$ndB acoustical shielding. To distinguish RPW signals from background\$$\backslash$backslash\$nnoise, recordings first were analyzed to identify larval sound impulse\$$\backslash$backslash\$nbursts, trains of 7-199 impulses, 3-30-ms in duration, where impulses\$$\backslash$backslash\$nwithin the train were separated by less than 0.25 s. For a burst to be\$$\backslash$backslash\$nconsidered a larval sound, it was specified that a majority of its\$$\backslash$backslash\$nimpulses must have spectra that match mean spectra (profiles) of known\$$\backslash$backslash\$nlarval sound impulses more closely than profiles of background noise or\$$\backslash$backslash\$nknown nontargeted sound sources. Based on these analyses, RPW larval\$$\backslash$backslash\$nbursts were detected in $\backslash$textgreater80\% of palm fronds inoculated with neonates\$$\backslash$backslash\$nthe previous day. There were no significant differences between burst\$$\backslash$backslash\$nrates in enclosed and open environments, but the shielding provided by\$$\backslash$backslash\$nthe enclosure enabled detection of early instars from greater distances.\$$\backslash$backslash\$nThus, there is potential to use acoustic technology to detect early RPW\$$\backslash$backslash\$ninfestation in either minimally shielded or open environments. In\$$\backslash$backslash\$naddition, because late-instar impulses ranged to higher amplitude and\$$\backslash$backslash\$nhad greater diversity of spectral features than with early instars, it\$$\backslash$backslash\$nmay be possible to identify late-instar infestations based on the\$$\backslash$backslash\$namplitudes and the diversity of sound features detected.},
  number = {4},
  journal = {Florida Entomol.},
  author = {Herrick, Nathan J and Mankin, R W},
  year = {2012},
  keywords = {invasive species,red palm weevil,caribbean,pest management},
  pages = {983--990}
}

