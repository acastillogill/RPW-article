@article{ponsExpansionAphidMyzocallis2006,
  title = {Expansion of the Aphid {{Myzocallis}} ({{Lineomyzocallis}}) Walshii ({{Monell}}) on the Red Oak {{Quercus}} Rubra, and Adaptation of Local Parasitoids in the Northeastern {{Iberian Peninsula}} ({{Hom}}., {{Aphididae}}, {{Calaphidinae}}; {{Hym}}., {{Braconidae}}, {{Aphidiinae}})},
